
{
  
  "rules": {
    "react/prop-types": 0,
    "indent": ["error", 2],
   "linebreak-style": 0,
   "global-require": 0,
   "eslint-linebreak-style": 0,
    "quotes": ["error", "double"]
  },
    "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "requireConfigFile": false
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
 
}